.login-person {background:url(/ep/luxor/ref/image/blue_theme/login/icon_login.png) left no-repeat; padding:5px 18px; line-height:18px}

/* login error 화면 */
.login-error {color:#393939; margin:0px; padding:50px;}
.login-box-error {padding:30px 50px; margin:auto;border-top:1px solid #8dbad8;}

/* login 말풍선 */
.tbl-talk01 {padding:0px; margin:0px;}
.tbl-talk01 td{padding:0px; margin:0px; font:11px Dotum,Verdana; color:#2d5591; line-height:14px;}
.tbl-talk01 .tl {width:10px; height:19px; background:url(/ep/luxor/ref/image/blue_theme/login/talkbox_01_tl.gif) left bottom no-repeat; font-size:0;}
.tbl-talk01 .t {height:19px; background:url(/ep/luxor/ref/image/blue_theme/login/talkbox_01_t.gif) left bottom repeat-x; font-size:0; text-align:left}
.tbl-talk01 .tr {width:10px; height:19px; background:url(/ep/luxor/ref/image/blue_theme/login/talkbox_01_tr.gif) right bottom no-repeat; font-size:0;}
.tbl-talk01 .bgl {background:url(/ep/luxor/ref/image/blue_theme/login/talkbox_01_l.gif) left top repeat-y;}
.tbl-talk01 .txt {padding:0px; background-color:#dfebf6;}
.tbl-talk01 .bgr {background:url(/ep/luxor/ref/image/blue_theme/login/talkbox_01_r.gif) right top repeat-y;}
.tbl-talk01 .bl {height:10px; background:url(/ep/luxor/ref/image/blue_theme/login/talkbox_01_bl.gif) left top no-repeat; font-size:0;}
.tbl-talk01 .b {background:url(/ep/luxor/ref/image/blue_theme/login/talkbox_01_b.gif) repeat-x; font-size:0;}
.tbl-talk01 .br {background:url(/ep/luxor/ref/image/blue_theme/login/talkbox_01_br.gif) right top no-repeat; font-size:0;}
.tbl-talk01 .point-area {position:relative; height:100%}
.tbl-talk01 .point-area .point-img {position:absolute; top:0px; left:15px; width:15px; height:19px; background:url(/ep/luxor/ref/image/blue_theme/login/talkbox_01_point.gif) left bottom no-repeat;}
.tbl-talk01 .stress {font-weight:bold}

/* 말풍선-버튼 */
.closed-btn * {display:inline-block; cursor:pointer; _cursor:hand;vertical-align:middle; }
.closed-btn {background:url(/ep/luxor/ref/image/blue_theme/login/btn_talk.gif) no-repeat left top; height:19px; line-height:19px;padding-left:8px; display:inline-block; }
.closed-btn a {display:inline-block; white-space:nowrap; background:url(/ep/luxor/ref/image/blue_theme/login/btn_talk.gif) no-repeat right top; padding-right:8px; color:#393939; font:normal 11px/19px Dotum,Verdana; text-decoration:none;}
.closed-btn a:hover {color:#4E6C97; text-decoration:none;}


/* 빌트인 포틀릿 탭 */
.built-in-portlet-tab{ width:100%;height:28px; padding-top:2px;}
.built-in-portlet-tab li{float:left;margin-right:-1px;background:url(/ep/luxor/ref/image/portlet/default/bg_tab_right.gif) 100% 0 no-repeat}
.built-in-portlet-tab li span{display:block}
.built-in-portlet-tab li a{display:block;_float:left;height:15px;padding:7px 10px 2px 10px;background:url(/ep/luxor/ref/image/portlet/default/bg_tab_left.gif) 0 0 no-repeat;color:#666;line-height:14px;text-decoration:none}
.built-in-portlet-tab li a:hover{color:#333}
.built-in-portlet-tab li.selected{background-position:100% 0px}
.built-in-portlet-tab li.selected a{background-position:0 0px;color:#333;font-weight:bold;letter-spacing:-1px}

.built-in-portlet-black-bold {font:bold 12px/160% Dotum,Verdana; color:#393939;}
.built-in-portlet-black {font:12px/160% Dotum,Verdana; color:#393939;}
.built-in-portlet-gray-none {font:12px/160% Dotum,Verdana; color:#9d9d9d;}

.built-in-portlet-main { width:100%;}
.built-in-portlet-main li {list-style:none; margin:0; padding:2px 5px 0px 12px; width:100%;line-height:18px;background: url("../image/portlet/icon_portlet.gif") no-repeat left center;}
.built-in-portlet-main li a:link, .portlet-main li a:visited {color:#555555; letter-spacing:0; text-decoration:none;}
.built-in-portlet-main li a:hover, .portlet-main li a:active  {color:#2f5081;}
.built-in-portlet-main span a:link, .portlet-main span a:visited {color:#5d7394; letter-spacing:0; text-decoration:none; font-size:11px;}
.built-in-portlet-main span a:hover, .portlet-main span a:active  {color:#5d7394; text-decoration:underline;}

.built-in-portlet-page {width:100%; padding-top:5px; padding-left:3px; }
.built-in-portlet-page a:link, .portlet-page a:visited {color:#555555; text-decoration:none;font-size:11px;}
.built-in-portlet-page a:hover, .portlet-page a:active  {color:#2f5081;}

.built-in-icon_arrow01{width:8px; height:8px;}
.built-in-icon_arrow02{width:8px; height:8px;}
