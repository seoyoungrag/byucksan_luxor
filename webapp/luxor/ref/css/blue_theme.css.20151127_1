/* dropdown menu style */
.top-sub-menu {z-index:1000;margin-top:7px;}
.top-sub-menu li {clear:both;padding:0 5px 5px 5px;}

ul.dropdown {z-index:1000;}
ul.dropdown li {}
ul.dropdown li.hover, ul.dropdown li:hover {color:black;position:relative;}
ul.dropdown li.hover a {color:black;}
ul.dropdown ul {background-color:#FCFCFC;border:1px solid #091e89;position:absolute;visibility:hidden;width:150px;}
ul.dropdown ul li {}
ul.dropdown ul li a {border-right:medium none;color:#666666;display:inline-block;width:100%;}
ul.dropdown ul li a:hover {color:#000000;text-decoration:none;}
ul.dropdown ul li a:active {color:#000000;text-decoration:none;}
ul.dropdown ul ul {left:100%;top:0;}
ul.dropdown li:hover > ul {visibility:visible;}

#mzTop.dropdown ul {background-color:#FCFCFC;border:1px solid #091e89;position:absolute;width:150px;left:-120px;padding-top:10px;}
#mzTop.dropdown ul li {width:90%;}
#mzTop.dropdown ul li a {padding:0px 5px 5px 5px}
#mzTop.dropdown ul ul {left:-100%;top:0;}

