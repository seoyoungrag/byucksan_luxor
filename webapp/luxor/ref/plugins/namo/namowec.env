[Product]
//Name = Samsung Enterprise Portal (MySingle)
Name = ACUBE

[UI]
ShowMenu = No
ShowPopupMenu = Yes
ShowStatus = No
//DeleteCommand = file_new, undo, redo, table_erase, table_draw,mark_para, mark_space,mark_tag,html_cleanup, bookmark, print, print_preview, helps, layer, mark_layer, layer_list, bgimage, file_info, advances, file_property
//CreateToolbar =  file_property | Menu FileOpen | font_list fontsize_list lineheight | char_bold char_italic char_underline char_color char_bgcolor char_clear | align_left align_center align_right | Hyperlink Image TableRibbon | ListNumber ListBullet IndentDecrease IndentIncrease
DeleteCommand = file_new, file_open, mark_para, mark_space,mark_tag,html_cleanup, bookmark, print, print_preview, helps, layer, mark_layer, layer_list, bgimage, file_info, advances, file_property, file_insert
CreateToolbar =  file_property | Menu font_list fontsize_list lineheight | char_bold char_italic char_underline char_color char_bgcolor char_clear | CreateToolbar = align_left align_center align_right | Hyperlink Image | TableRibbon TableDraw TableErase | ListNumber ListBullet IndentDecrease IndentIncrease;
//CreateToolbar =  file_property | Menu font_list fontsize_list lineheight | char_bold char_italic char_underline char_color char_bgcolor char_clear ; CreateToolbar = align_left align_center align_right | Hyperlink Image | TableRibbon TableDraw TableErase | ListNumber ListBullet IndentDecrease IndentIncrease;
//ShowTab = No
//CreateTab = Source
Border = None
HyperLinkDefaultTarget = _blank
AnswerAskDialog = SaveAsUnicode:Yes;

[Document]
NewFile = "newFile.htm"
DefaultFont = ,10,u,10
LeftMargin = 0
TopMargin = 0

[Image]
#FileFilterString = "그림 파일(*.gif *.jpg *.jpeg *.png)|*.gif;*.jpg;*.jpeg;*.png|JPEG 파일(*.jpg *.jpeg)|*.jpg;*.jpeg|GIF 파일(*.gif)|*.gif|PNG 파일(*.png)|*.png|모든 파일|*.*|"
ImageWidthLimit = Msg,,800

[Print]
OddEvenDifferent = Yes
OddPageLeftHeader = ""
OddPageRightHeader = ""
OddPageLeftFooter = ""
OddPageRightFooter = ""
EvenPageLeftHeader = ""
EvenPageRightHeader = ""
EvenPageLeftFooter = ""
EvenPageRightFooter = ""
BgImage = Yes
BlackText = No
PaginateInTable = Yes

[MIME]
//EncodeRange = Body
EncodeRange = All
EncodeFileScope = Local
EncodeFilter = Image
EncodeType = Base64

[Event]
Keyboard = Yes