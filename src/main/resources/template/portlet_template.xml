<portlet-app
    xmlns="http://acube.sds.co.kr/xml/ns/portlet/portlet-app_1_0.xsd"
    version="1.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <portlet>
        <description>%DESCRIPTION%</description>
        <portlet-context-name>%PORTLET-CONTEXT-NAME%</portlet-context-name>
        <display-name>%DISPLAY-NAME%</display-name>
        <supports>
            <mime-type>text/html</mime-type>
        </supports>
        <portlet-info>
            <title>%TITLE%</title>
            <login-flag>%LOGIN-FLAG%</login-flag>
            <sso-info>%SSO-INFO%</sso-info>
            <url-info>
                <view><![CDATA[%VIEW%]]></view>
                <edit><![CDATA[%EDIT%]]></edit>
                <help><![CDATA[%HELP%]]></help>
                <go><![CDATA[%GO%]]></go>
                <img><![CDATA[%IMG%]]></img>                
            </url-info>
            <resource-info>
                <javascript>%JAVASCRIPT%</javascript>
                <style>%STYLE%</style>
            </resource-info>
        </portlet-info>
    </portlet>
</portlet-app>