<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
      http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="attachmentService" class="com.sds.acube.luxor.common.service.impl.AttachmentServiceImpl">
        <property name="dao" ref="attachmentDAO" />
    </bean>
    
    <bean id="attachmentDAO" class="com.sds.acube.luxor.common.dao.AttachmentDAO">
        <property name="queryService" ref="queryService" />
    </bean>
    
</beans>
