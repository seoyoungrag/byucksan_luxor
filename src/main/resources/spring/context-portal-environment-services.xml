<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
      http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="portalEnvironmentService" class="com.sds.acube.luxor.portal.service.impl.PortalEnvironmentServiceImpl">
        <property name="portalEnvironmentDAO" ref="portalEnvironmentDAO" />
    </bean>
    
    <bean id="portalEnvironmentDAO" class="com.sds.acube.luxor.portal.dao.PortalEnvironmentDAO">
        <property name="queryService" ref="queryService" />
    </bean>
    
</beans>
