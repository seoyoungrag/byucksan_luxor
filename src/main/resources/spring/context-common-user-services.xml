<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
      http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="userService" class="com.sds.acube.luxor.common.service.impl.UserServiceImpl">
        <property name="orgService" ref="wsClientOrgProviderService"/>   
        <property name="aclService" ref="wsClientAclProviderService"/>   
    </bean>
    
    <bean id="orgService" class="com.sds.acube.luxor.common.service.impl.OrgServiceImpl">
        <property name="orgService" ref="wsClientOrgProviderService"/>
        <property name="aclService" ref="wsClientAclProviderService"/>
    </bean>
    
</beans>
