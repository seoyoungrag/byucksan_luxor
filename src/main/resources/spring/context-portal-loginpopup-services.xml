<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
      http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="loginPopupService" class="com.sds.acube.luxor.portal.service.impl.LoginPopupServiceImpl">
        <property name="loginPopupDAO" ref="loginPopupDAO" />
    </bean>
    
    <bean id="loginPopupDAO" class="com.sds.acube.luxor.portal.dao.LoginPopupDAO">
        <property name="queryService" ref="queryService" />
    </bean>
    
</beans>
