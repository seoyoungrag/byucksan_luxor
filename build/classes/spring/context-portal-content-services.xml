<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
      http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="portalContentService" class="com.sds.acube.luxor.portal.service.impl.PortalContentServiceImpl">
        <property name="messageService" ref="messageService" />
        <property name="portalPageZoneService" ref="portalPageZoneService" />
        <property name="portalContentDAO" ref="portalContentDAO" />
    </bean>
    
    <bean id="portalContentDAO" class="com.sds.acube.luxor.portal.dao.PortalContentDAO">
        <property name="queryService" ref="queryService" />
    </bean>
</beans>
