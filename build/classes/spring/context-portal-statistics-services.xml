<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
      http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="statisticsService" class="com.sds.acube.luxor.common.service.impl.StatisticsServiceImpl">
        <property name="statisticsDAO" ref="statisticsDAO" />
    </bean>
    
    <bean id="statisticsDAO" class="com.sds.acube.luxor.common.dao.StatisticsDAO">
        <property name="queryService" ref="queryService" />
    </bean>
    
</beans>
