<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
      http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="loginService" class="com.sds.acube.luxor.common.service.impl.IDIRLoginServiceImpl">
        <property name="orgService" ref="wsClientOrgProviderService"/>
        <property name="loginDAO" ref="loginDAO" />
    </bean>

    <bean id="loginDAO" class="com.sds.acube.luxor.common.dao.LoginDAO">
        <property name="queryService" ref="queryService" />
    </bean>
    
</beans>
